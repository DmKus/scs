hypermedia_nrel_definition->15293;;
15293=>main_ru_idtf:[Опр.(треугольник)];;
15293->rrel_defined_notion:11937;;
15293=>hypermedia_nrel_translation:[Треугольник - это геометрическая фигура, которая является результатом объединения понятия линейного треугольника и множества всех точек плоскости, которые лежат между двух любых точек, принадлежащих этому линейному треугольнику.](*<-lang_ru;;*);;
15293=>nrel_used_constants:
{
	20640;
	9792;
	20632;
	5501
};;

/*Геометрия:Предметная область Геометрии Евклида понятия 4-го логического уровня_*/

nrel_state_definition->15287;;
15287=>main_ru_idtf:[Утв.(треугольник)];;
15287=>hypermedia_nrel_translation:[Многоугольник является треугольником в том случае, если у него три стороны.](*<-lang_ru;;*);;

nrel_single_assignment_statement->51352;;
51352=>main_ru_idtf:[Утв.(треугольник; непрямолинейная тройка точек; вершина*)];;
51352=>hypermedia_nrel_translation:[Треугольник однозначно задается непрямолинейной тройкой точек, каждая из которых является вершиной этого треугольника.](*<-lang_ru;;*);;
51352=>hypermedia_nrel_translation:[Для каждой непрямолинейной тройки точек существует единственный треугольник, вершинами которых являются указанные три точки.](*<-lang_ru;;*);;

